{
    "name": "p2t-browser",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "webpack -w --config ./config/webpack.dev.js",
        "prod": "cross-env NODE_ENV=prod webpack -w --config ./config/webpack.dev.js",
        "dev:firefox": "cross-env BROWSER=firefox webpack -w --config ./config/webpack.dev.js",
        "prod:firefox": "cross-env NODE_ENV=prod BROWSER=firefox webpack -w --config ./config/webpack.dev.js",
        "build": "yarn build:dev && yarn build:prod && yarn build:firefox_dev && yarn build:firefox_prod",
        "build:dev": "cross-env BUILD=true webpack --config ./config/webpack.prod.js",
        "build:prod": "cross-env BUILD=true NODE_ENV=prod webpack --config ./config/webpack.prod.js",
        "build:firefox_dev": "cross-env BUILD=true BROWSER=firefox webpack --config ./config/webpack.prod.js",
        "build:firefox_prod": "cross-env BUILD=true NODE_ENV=prod BROWSER=firefox webpack --config ./config/webpack.prod.js",
        "create:safari_dev": "yarn build:firefox_dev && xcrun safari-web-extension-converter ./build/firefox/dev --app-name safari-dev",
        "create:safari_prod": "yarn build:firefox_prod && xcrun safari-web-extension-converter ./build/firefox/prod --app-name safari-prod",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "dev:Firefox2": "web-ext run -s ./dist --firefox-preview",
        "build:Firefox2": "web-ext build -s ./dist -a ./dist-firefox"
    },
    "keywords": [
        "react",
        "typescript",
        "chrome",
        "extension",
        "boilerplate"
    ],
    "author": "",
    "license": "",
