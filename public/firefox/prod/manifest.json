{
    "manifest_version": 2,
    "name": "",
    "description": "",
    "version": "",
    "default_locale": "",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.com/**"
            ],
            "js": [
                "js/contentScriptMain.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "browser_action": {
        "default_icon": "icon-48.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/backgroundPage.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "*://*/*",
        "tabs"
    ]
}